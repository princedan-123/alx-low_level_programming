# creates an executable called school and explicitly performs clean ups
# compiler
CC=gcc
# C source codes
SRC= main.c school.c
# object codes
OBJ=$(SRC:.c=.o)



NAME=school

all: $(OBJ)
	$(CC) $(CFLAGS) $(OBJ) -o $(NAME)

.c.o:
	$(CC) $(CFLAGS) -c $< -o $@

$(SRC): m.h

clean:
	$(RM) *.~ *.swp $(NAME)

oclean:
	$(RM) *.o

fclean: clean oclean

re: all fclean
