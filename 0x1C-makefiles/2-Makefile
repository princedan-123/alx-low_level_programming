# builds the target file school
# CC variable for the compiler
# SRC variable for all files with .c extension
# OBJ variable for all file with .o extension

all: $(NAME)

CC=gcc
SRC= main.c school.c
OBJ=main.o school.o
NAME=school
$(NAME): $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

%.o: %.c
	$(CC) -c $^ -o $@
